<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tictactoy">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TicTacToy">
        <activity android:name=".SingleGameActivity"></activity>
        <activity android:name=".MultiplayerNameActivity" />
        <activity android:name=".SinglePlayerNameActivity" />
        <activity android:name=".GameMode_Activity" />
        <activity android:name=".Game_Activity" />
        <activity android:name=".Wining_Activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>